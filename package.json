{
  "name": "@sesamy/sesamy-components",
  "description": "Ahareable web components library with Vite + Svelte",
  "version": "0.0.2",
  "type": "module",
  "module": "dist/lib/sesamy-components.js",
  "main": "dist/lib/sesamy-components.umd.js",
  "types": "dist/lib/sesamy-components.d.ts",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "vite --config vite.demo.config.ts",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build -s"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.5.5",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.2",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.1"
  },
  "files": [
    "dist/lib"
  ],
  "dependencies": {
    "@sesamy/sesamy-js": "^1.21.6",
    "lucide-svelte": "^0.445.0"
  }
}
